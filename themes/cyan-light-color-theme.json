{
	"name": "Cyan light theme fork",
	"colors": {
		"foreground": "#1d1d1d",
		"activityBar.background": "#e4e6eb",
		"activityBar.foreground": "#1d1d1d",
		"editor.background": "#F2F3F7",
		"editor.foreground": "#262626",
		"editor.selectionBackground": "#B8DDE0",
		"list.activeSelectionIconForeground": "#fff",
		"titleBar.activeBackground": "#e4e6eb",
		"titleBar.inactiveBackground": "#F2F3F7",
		"activityBar.border": "#bec5cd",
		"sideBar.border": "#bec5cd",
		"sideBar.background": "#e4e6eb",
		"tab.border": "#bec5cd",
		"tab.hoverBackground": "#b9bdc999",
		"tab.activeBorder": "#29abcb",
		"tab.inactiveBackground": "#e4e6eb",
		"scrollbarSlider.background": "#e4e6eb",
		"scrollbarSlider.activeBackground": "#ced2d9",
		"scrollbarSlider.hoverBackground": "#ced2d9",
		"menu.background": "#e4e6eb",
		"menu.selectionBackground": "#3eb2c2",
		"panelTitle.activeBorder": "#29abcb",
		"list.activeSelectionBackground": "#3eb2c2",
		"list.inactiveSelectionBackground": "#d0d5db"
	},
	"tokenColors": [
		{
			"scope": [
				"punctuation.brackets.curly",
				"entity.name.type.struct",
				"entity.name.type.declaration",
				"keyword.other.type.php",
				"meta.class.body",
				"support.class.php",
			],
			"settings": {
				"foreground": "#000000",
			}
		},
		{
			"scope": [
				"entity.name.function.macro.rust",
				"keyword.operator.macro.dollar.rust",
			],
			"settings": {
				"foreground": "#DD6718",
			}
		},
		{
			"scope": [
				"string.quoted.double",
				"string.quoted.single",
				"string.interpolated",
				"string"
			],
			"settings": {
				"foreground": "#00733B",
			}
		},
		{
			"scope": [
				"entity.name.type.option",
				// "entity.name.type",
				"constant.other.caps",
				"variable.other",
				"meta.function.call",
				"comment.block.documentation.phpdoc.php",
				"variable.language.this.php",
				"constant.other.class.php"
			],
			"settings": {
				"foreground": "#700000",
			}
		},
		{
			"scope": [
				"meta.attribute.rust",
			],
			"settings": {
				"foreground": "#808000",
			}
		},
		{
			"scope": [
				"storage.type.rust",
				"keyword.other.crate.rust",
				"keyword.other.rust",
				"variable.language.self",
				"keyword.control",
				"constant.character.escape",
				"storage.modifier",
				"keyword.declaration",
				"keyword.other.import",
				"support.function.construct",
				"constant.language",
				"storage.type.function",
				"meta.method-call.static"
			],
			"settings": {
				"foreground": "#000080",
			}
		},
		{
			"scope": [
				"entity.name.type.numeric",
				"constant.numeric.decimal",
				"constant.numeric",
				"keyword.other.use",
			],
			"settings": {
				"foreground": "#0073E6",
			}
		},
		{
			"scope": [
				"comment.line.documentation",
				"comment.line.double-slash",
				"keyword.other.phpdoc.php",
				"comment.block.documentation.phpdoc.php",
				"punctuation.definition.comment.php",
				"comment.line.number-sign.php"
			],
			"settings": {
				"foreground": "#808080",
			}
		},
		{
			"scope": [
				"entity.name.type.lifetime",
			],
			"settings": {
				"foreground": "#20999D",
			}
		},
		{
			"scope": [
				"support.class",
			],
			"settings": {
				"foreground": "#2196F3",
			}
		},
		{
			"scope": [
				
			],
			"settings": {
				"foreground": "#d9ff00",
			}
		},
	]
}